{

  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "0.72",
  "default_locale": "en",
  "browser_specific_settings": {
    "gecko": {
      "id": "SimpleAntiContainer@blackadderkate.com",
      "strict_min_version": "71.0"
    }
  },

  "background": {
    "page":  "background.html"
  },

  "options_ui": {
    "page": "options.html",
    "browser_style": true,
    "open_in_tab":true
  },

  "permissions": [
    "menus",
    "tabs",
    "contextMenus",
    "activeTab",
    "downloads",
    "webRequest",
    "storage",
    "alarms",
    "notifications",
    "nativeMessaging",
    "<all_urls>"
  ],

  "browser_action": {
  "browser_style": true,
  "default_icon": {
    "16": "icons/page_save_16x16.png",
    "32": "icons/page_save_32x32.png"
  },
  "default_title": "__MSG_extensionName__"
  },

  "icons": {
    "16": "icons/page_save_16x16.png",
    "32": "icons/page_save_32x32.png"
  }
}
